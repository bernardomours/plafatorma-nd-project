models:
    Appointment: { appointment_date: date, check_in: 'time nullable', check_out: 'time nullable', service_type: string, session_number: 'integer nullable', relationships: { belongsTo: 'Patient, Professional, Therapy, Unit' } }
    Patient: { name: string, birth_date: date, cpf: 'string unique', guardian_name: string, guardian_phone: string, relationships: { belongsTo: Unit, hasMany: 'Appointment, RequestedService' } }
    Professional: { name: string, cpf: 'string unique', phone: string, birth_date: date, register_number: 'string nullable', relationships: { hasMany: Appointment, belongsTo: Therapy } }
    RequestedService: { service_type: string, month_year: string, requisition_number: 'string nullable', requested_hours: time, approved_hours: time, planned_hours: time, relationships: { belongsTo: 'Patient, Therapy' } }
    Schedule: { day_of_week: string, start_time: time, end_time: time, relationships: { belongsTo: 'Patient, Professional, Therapy, Unit' } }
    Therapy: { name: string, relationships: { hasMany: 'Appointment, RequestedService' } }
    Unit: { name: string, cpnj: 'string unique', street: string, neighborhood: string, number: string, relationships: { hasMany: 'Professional, Patient, Appointment' } }
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
